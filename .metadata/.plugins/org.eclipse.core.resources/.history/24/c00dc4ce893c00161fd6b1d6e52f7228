import java.util.Scanner;

public class Kangaroo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int x1=sc.nextInt();
		int r1=sc.nextInt();
		int x2=sc.nextInt();
		int r2=sc.nextInt();
		if(x2>x1&&r2>r1){
			System.out.println("NO");
		}
		else 
			System.out.println("YES");
		

	}

}
